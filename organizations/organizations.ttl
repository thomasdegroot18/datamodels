@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix haOrg: <https://data.hetarchief.be/ns/organization/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix edtf: <http://id.loc.gov/datatypes/edtf/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix pav: <http://purl.org/pav/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

@base <https://data.hetarchief.be/ns/organization> .

<>  a owl:Ontology;
    vann:preferredNamespacePrefix "haOrg" ;
    vann:preferredNamespaceUri "http://data.hetarchief.be/ns/organization/" ;
    dct:author  [
        schema:name   "Miel Vander Sande" ;
        schema:email  "miel.vandersande@meemoo.be"
    ] ,
                [
        schema:name   "Milan Valadou" ;
        schema:email  "milan.valadou@meemoo.be"
    ] ,
    [
        schema:name   "Lennert Van de Velde" ;
        schema:email  "lennert.vandevelde@meemoo.be"
    ] ;
    dct:title     "Data model Organizations"@en ;
    dct:title     "Modèle de données Organisations"@fr ;
    dct:title     "Datamodel Organisaties"@nl ;
    dct:created   "2022-03-14" ;
    dct:modified  "2023-01-05" ;
    dct:description """Data model to describe organizations."""@en ;
    dct:description """Modèle de données pour décrire des organisations."""@fr ;
    dct:description """Datamodel voor het beschrijven van organisaties."""@nl ;
    pav:version "0.0.1" .

#####################################
#              CLASSES              #
#####################################

haOrg:ContentPartner   a   rdfs:Class ;
    rdfs:subClassOf org:Organization ;  
    rdfs:label "Content Partner"@en ;
    rdfs:label "Partenaire de Contenus"@fr ;
    rdfs:label "Contentpartner"@nl ;
    skos:definition """A content partner is an organization that has signed a collaboration agreement with meemoo for the long-term preservation and dissemination of their content."""@en ;
    skos:definition """Un partenaire de contenu est une organisation qui a signé une convention de collaboration avec meemoo pour la préservation à long terme et la diffusion de leur contenu."""@fr ;
    skos:definition """Een contentpartner is een organisatie die een samenwerkingovereenkomst heeft ondertekend met meemoo voor de langetermijnpreservatie en ontsluiting van hun materiaal."""@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .
    # `Aanbieder` wordt gebruikt in AVO voor CP

haOrg:EducationalOrganization  a   rdfs:Class ;
    rdfs:subClassOf org:Organization ;
    owl:equivalentClass schema:EducationalOrganization ;
    rdfs:label "Educational Organization"@en ;
    rdfs:label "Organisation Éducative"@fr ;
    rdfs:label "Educatieve organisatie"@nl ;
    skos:definition """An educational organization is an organization or company that provides educational services outside the school context."""@en ;
    skos:definition """Une organisation éducative est une organisation ou une entreprise qui fournit des services éducatifs hors du contexte scolaire."""@fr ;
    skos:definition """Een educatieve organisatie is een organisatie of een bedrijf dat educatie en opleiding aanbiedt buiten een schoolcontext."""@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

haOrg:EducationalPartner   a   rdfs:Class ;
    rdfs:subClassOf haOrg:EducationalOrganization ;
    rdfs:label "Educational Partner"@en ;
    rdfs:label "Partenaire Éducatif"@fr ;
    rdfs:label "Educatieve partner"@nl ;
    skos:definition """An educational partner is an organization that provides content on the 'Archief voor Onderwijs' platform and has signed the 'educational partner' collaboration agreement with meemoo (e.g. an educational publisher, KlasCement, Mediawijs, etc.)."""@en ;
    skos:definition """Un partenaire éducatif est une organisation qui fournit du contenu sur la plateforme 'Archief voor Onderwijs' et a signé avec meemoo l'accord de collaboration 'partenaire éducatif' (par exemple, un éditeur scolaire, KlasCement, Mediawijs, etc.)."""@fr ;
    skos:definition """Een educatieve partner is een organisatie die content aanbiedt op het 'Archief voor Onderwijs'-platform en met meemoo de samenwerkingsovereenkomst 'educatieve partner' heeft ondertekend (bv. een educatieve uitgeverij, KlasCement, Mediawijs, enz.)."""@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

haOrg:EducationalPublisher a   rdfs:Class ;
    rdfs:subClassOf haOrg:EducationalOrganization ;
    rdfs:label "Educational Publisher"@en ;
    rdfs:label "Éditeur éducatif"@fr ;
    rdfs:label "Educatieve uitgeverij"@nl ;
    skos:definition """An educational publisher is a company that publishes educational content."""@en ;
    skos:definition """Un éditeur scolaire est une entreprise qui publie des contenus pédagogiques."""@fr ;
    skos:definition """Een educatieve uitgeverij is een bedrijf dat educatieve content publiceert."""@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

haOrg:Logo a   rdfs:Class ;
    rdfs:label  "Logo"@en ;
    rdfs:label  "Logo"@fr ;
    rdfs:label  "Logo"@nl ;
    skos:definition """A logo is a graphic mark, emblem, or symbol of an organization."""@en ;
    skos:definition """Un logo est une marque, un emblème ou un symbole graphique d'une organisation."""@fr ;
    skos:definition """Een logo is een grafisch teken, embleem of symbool van een organisatie."""@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

# haOrg:LogoContext  a   rdfs:Class ;
#     rdfs:subClassOf skos:Concept ;
#     rdfs:label  "Context of a Logo"@en ;
#     rdfs:label  "Contexte d'un Logo"@fr ;
#     rdfs:label  "Context van een Logo"@en ;
#     rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

haOrg:OrganizationType a  rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label  "Organization Type"@en ;
    rdfs:label  "Type de Organisation"@fr ;
    rdfs:label  "Organisatietype"@nl ;
    skos:definition """The type or classification of an organization."""@en ;
    skos:definition """Le type ou la classification d'une organisation."""@fr ;
    skos:definition """Het type of de classificatie van een organisatie."""@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

haOrg:School   a   rdfs:Class ;
    rdfs:subClassOf haOrg:EducationalOrganization ;
    owl:equivalentClass schema:School ;
    rdfs:label "School"@en ;
    rdfs:label "École"@fr ;
    rdfs:label "School"@nl ;
    skos:definition """A school is an educational institution (recognized by the Flemish government) in primary, secondary or higher education."""@en ;
    skos:definition """Une école est une institution éducative (reconnue par le gouvernement flamand) dans l'enseignement primaire, secondaire ou supérieur."""@fr ;
    skos:definition """Een door-de-Vlaamse-overheid-erkende onderwijsinstelling in het basis, secundair of hoger onderwijs."""@nl;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

haOrg:ServiceConsumer  a   rdfs:Class ;
    rdfs:subClassOf org:Organization ;
    rdfs:label "Service Consumer"@en ;
    rdfs:label "Consomatteur de Services"@fr ;
    rdfs:label "Dienstenafnemer"@nl ;
    skos:definition """An organization that consumes services from meemoo, with a contractual relationship with meemoo. This is not a content partner, but a private organization that stores digital content at meemoo (e.g. Plantentuin Meise)."""@en ;
    skos:definition """Une organisation qui consomme des services de meemoo, avec une relation contractuelle avec meemoo. Ce n'est pas un partenaire de contenu, mais une organisation privée qui stocke du contenu numérique chez meemoo (par exemple Plantentuin Meise)."""@fr ;
    skos:definition """Een organisatie die diensten bij meemoo afneemt, met contractuele relatie met meemoo. Het gaat hier niet over een contentpartner, maar over een particuliere organisatie die digitale opslag doet bij meemoo (bv. Plantentuin Meise)."""@nl;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

haOrg:ServiceProvider  a   rdfs:Class ;
    rdfs:subClassOf org:Organization ;
    rdfs:label "Service Provider"@en ;
    rdfs:label "Fournisseur de Services"@fr ;
    rdfs:label "Dienstenleverancier"@nl ;
    skos:definition """An organization that provides services or goods to meemoo, for example related to IT, digitization, etc."""@en ;
    skos:definition """Une organisation qui fournit des services ou des biens à meemoo, par exemple dans le domaine de l'informatique, de la numérisation, etc."""@fr ;
    skos:definition """Een organisatie die aan meemoo diensten of goederen levert, bijvoorbeeld op het gebied van IT, digitalisering, enz."""@nl;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

#####################################
#             PROPERTIES            #
#####################################

haOrg:isAccountManagerOf a   rdf:Property ;
    rdfs:domain schema:Person ;
    rdfs:range org:Organization ;
    owl:inverseOf haOrg:hasAccountManager ;
    rdfs:label "Is account manager of"@en ;
    rdfs:label "Est gestionnaire de comptes de"@fr ;
    rdfs:label "Is account manager van"@nl ;
    skos:definition "An organization that a person is the account manager of."@en ;
    skos:definition "Une organisation dont une personne est le gestionnaire de comptes."@fr ;
    skos:definition "Een organisatie waarvan een persoon de accountmanager is."@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

# haOrg:applicationContext   a   rdf:Property ;
#     rdfs:domain haOrg:Logo ;
#     rdfs:range haOrg:LogoContext ;
#     rdfs:label "Context in which a logo is applied"@en ;
#     rdfs:label "Contexte dans lequel un logo est utilisé"@fr ;
#     rdfs:label "Context waarin een logo gebruikt wordt"@nl ;
#     rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

haOrg:hasAccountManager    a   rdf:Property ;
    rdfs:domain org:Organization ;
    rdfs:range  schema:Person ;
    owl:inverseOf haOrg:isAccountManagerOf ;
    rdfs:label "has an account manager"@en ;
    rdfs:label "a un gestionnaire de comptes"@fr ;
    rdfs:label "heeft een accountmanager"@nl ;
    skos:definition "A person that is the account manager of an organization."@en ;
    skos:definition "Une personne qui est le gestionnaire de comptes d'une organisation."@fr ;
    skos:definition "Een persoon die de accountmanager is van een organisatie."@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

haOrg:hasLogo  a   rdf:Property ;
    rdfs:domain org:Organization ;
    rdfs:range  haOrg:Logo ;
    rdfs:label "has a logo"@en ;
    rdfs:label "a un logo"@fr ;
    rdfs:label "heeft een logo"@nl ;
    skos:definition "A logo for an organization."@en ;
    skos:definition "Un logo pour une organisation."@fr ;
    skos:definition "Een logo voor een organisatie."@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/organization/> .

#####################################
#               SHAPES              #
#####################################

######################
#     NodeShapes     #
######################

<#OrganizationShape> a sh:NodeShape ;
  sh:targetClass org:Organization ;
  sh:property [ 
    a sh:PropertyShape ;
    sh:path skos:prefLabel ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype rdf:langString ;
      
    sh:name "preferred label"@en ;
    sh:name "label préféré"@fr ;
    sh:name "voorkeursnaam/-label"@nl ;

    sh:description "De voorkeursnaam/-label van de organisatie."@nl ;
    sh:description "The preferred name/label of the organization."@en ;
    sh:description "Le nom / étiquette préféré de l'organisation."@fr ;
      
    sh:message "is missing or its Object is no language string."@en ;
  ],[ 
    a sh:PropertyShape ;
    sh:path skos:altLabel ;
    sh:datatype rdf:langString ;

    sh:description "The alternative label of the organization."@en ;
    sh:description "Le label alternatif de l'organisation."@fr ;
    sh:description "Het alternatieve label van de organisatie."@nl ;

    sh:name "alternative label"@en ;
    sh:name "label alternatif"@fr ;
    sh:name "alternatieve naam/label"@nl ;
      
    sh:message "is no langString"@en
  ],
  [
    sh:path haOrg:hasLogo ;
    sh:class haOrg:Logo ;

    sh:name "logo"@en ;
    sh:name "logo"@fr ;
    sh:name "logo"@nl ;

    sh:description "A logo associated with the organization."@en ;
    sh:description "Un logo associé à l'organisation."@fr ;
    sh:description "Een logo dat geassocieerd is met de organisatie."@nl ;
    
    sh:severity sh:Violation;
    sh:message "is no haOrg:Logo"@en ;
  ], 
  [
    sh:path foaf:homepage ;
    sh:maxCount 1 ;
    sh:nodeKind sh:IRI ;
    
    sh:name "homepage"@en ;
    sh:name "page d'accueil"@fr ;
    sh:name "homepagina"@nl ;

    sh:description "De website van de organisatie."@nl ;
    sh:description "The website of the organization."@en ;
    sh:description "Le site Web de l'organisation."@fr ;
    
    sh:message "occurs more than once or its object is no valid IRI"@en ;
  ],
  [
    sh:path dct:description ;
    sh:maxCount 1 ;
    sh:nodeKind sh:Literal ;
    sh:datatype rdf:langString ;
    
    sh:name "description"@en ;
    sh:name "description"@fr ;
    sh:name "beschrijving"@nl ;
    
    sh:description "Description of the organization."@en ;
    sh:description "Description de l'organization."@fr ;
    sh:description "Beschrijving van de organisatie."@nl ;

    sh:message "is not of type xsd:string or occurs more than once"@en ;
  ],
  [
    sh:path org:classification ;
    sh:class haOrg:OrganizationType ;

    sh:name "classification"@en ;
    sh:name "classification"@fr ;
    sh:name "classificatie"@nl ;

    sh:description "Het type waartoe de organisatie behoort."@nl ;
    sh:description "The type to which the organization belongs."@en ;
    sh:description "Le type auquel appartient l'organisation."@fr ;

    sh:message "is no haOrg:OrganizationType"@en ;
  ],
  [
    sh:path org:hasUnit ;
    sh:class org:OrganizationalUnit ;

    sh:name "has Unit"@en ;
    sh:name "possède une Unité"@fr ;
    sh:name "heeft een eenheid"@nl ;

    sh:description "Een aparte eenheid of afdeling van de organisatie."@nl ;
    sh:description "A separate unit or department of the organization."@en ;
    sh:description "Une unité ou un département distinct de l'organisation."@fr ;

    sh:message "is no org:OrganizationalUnit"@en ;
  ],
  [
    sh:path schema:contactPoint ;
    sh:class schema:ContactPoint ;
    
    sh:name "contact point"@en ;
    sh:name "point de contact"@fr ;
    sh:name "aanspreekpunt"@nl ;

    sh:description "Een organisatie kan een aanspreekpunt hebben."@nl ;
    sh:description "An organization can have a point of contact."@en ;
    sh:description "Une organisation peut avoir un point de contact."@fr ;
    
    sh:message "is no schema:ContactPoint"@en ;
  ],
  [
    sh:path org:hasSite ;
    sh:class org:Site ;
    
    sh:name "has site"@en ;
    sh:name "a un site"@fr ;
    sh:name "heeft vestiging"@nl ;

    sh:description "Een fysieke of virtuele vestiging van de organisatie."@nl ;
    sh:description "A physical or virtual location of the organization."@en ;
    sh:description "Un emplacement physique ou virtuel de l'organisation."@fr ;
    
    sh:message "occurs more than once or its Object is no org:Site"@en ;
  ],
  [
    sh:path org:hasPrimarySite ;
    sh:maxCount 1 ;
    sh:class org:Site ;
    
    sh:name "has primary site"@en ;
    sh:name "a un site principal"@fr ;
    sh:name "heeft primaire vestiging"@nl ;

    sh:description "De primaire vestiging van de organisatie."@nl ;
    sh:description "The primary location of the organization."@en ;
    sh:description "L'emplacement principal de l'organisation."@fr ;
    
    sh:message "occurs more than once or its value is no org:Site"@en 
  ],
  [
    sh:path haOrg:hasAccountmanager ;
    sh:maxCount 1 ;
    sh:class schema:Person ;

    sh:name "has account manager"@en ;
    sh:name "a gestionnaire de comptes"@fr ;
    sh:name "heeft account manager"@nl ;

    sh:description "De persoon binnen een organisatie die verantwoordelijk is voor het contact met een andere organisatie, bv. een klant."@nl ;
    sh:description "La personne au sein d'une organisation responsable du contact avec une autre organisation, par exemple un client."@fr ;
    sh:description "The person within an organization that is responsible for contact with another organization, e.g., a customer."@en ;

    sh:message "occurs more than once or its value is no schema:Person"@en 
  ],
  [
    sh:path org:hasPost ;
    sh:class org:Post ;

    sh:name "has post"@en ;
    sh:name "possède un poste"@fr ;
    sh:name "heeft positie"@nl ;
    
    sh:description "Indicates a Post which exists within the organization."@en ;
    sh:description "Indique un Poste qui existe dans l'organisation."@fr ;
    sh:description "Verwijst naar een positie binnen de organisatie."@nl ;

    sh:message "is no org:Post"@en ;
  ] .

haOrg:OrganizationalUnitShape a sh:NodeShape ;
  sh:targetClass org:OrganizationalUnit ;
  sh:property [
    sh:path org:unitOf ;
    sh:minCount 1 ;
    sh:class org:Organization ;

    sh:name "unit of"@en ;
    sh:name "unité de"@fr ;
    sh:name "organisatie-eenheid van"@nl ;

    sh:description "De organisatie waarvan de organisatie-eenheid deel uitmaakt."@nl ;
    sh:description "The organization of which the organizational unit is part."@en ;
    sh:description "L'organisation dont l'unité organisationnelle fait partie."@fr ;

    sh:message "is missing or is no org:Organization."@en ;
  ] .

<#ContentPartnerShape> a sh:NodeShape ;
  sh:targetClass haOrg:ContentPartner ;
  sh:property [
    sh:path haOrg:hasAccountManager ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:class schema:Person ;
      
    sh:name "has account manager"@en ;
    sh:name "a gestionnaire de comptes"@fr ;
    sh:name "heeft account manager"@nl ;

    sh:description "The account manager of the content partner."@en ;
    sh:description "Le gestionnaire de comptes de l'contentpartner."@fr ;
    sh:description "De account manager van de contentpartner."@nl ;
    
    sh:message "is missing, occurs more than once or is no schema:Person"@en ;
  ] .

<#EducationalPartnerShape>  a sh:NodeShape ;
  sh:targetClass haOrg:EducationalPartner ;
  sh:property [
    sh:path haOrg:hasAccountManager ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:class schema:Person ;
      
    sh:name "has account manager"@en ;
    sh:name "a gestionnaire de comptes"@fr ;
    sh:name "heeft account manager"@nl ;

    sh:description "The account manager of the educational partner."@en ;
    sh:description "Le gestionnaire de comptes du partenaire éducatif."@fr ;
    sh:description "De account manager van de educatieve partner."@nl ;
      
    sh:message "is missing, occurs more than once or its Object is no schema:Person"@en ;
  ] .

<#SiteShape>  a  sh:NodeShape ;
  sh:targetClass org:Site ;
  sh:property [
    sh:path org:siteAddress ;
    sh:maxCount 1 ;
    sh:class schema:PostalAddress ;
    
    sh:name "site Address"@en ;
    sh:name "adresse du Site"@fr ;
    sh:name "adres van de vestiging"@nl ;

    sh:description "Het adres van de vestiging."@nl ;
    sh:description "The address of the location."@en ;
    sh:description "L'adresse de l'emplacement."@fr ;

    sh:message "org:siteAddress occurs more than once or its Object is no schema:PostalAddress"@en ;
    sh:message "org:siteAddress apparaît plus d'une seule fois ou son Object n'est pas un schema:PostalAddress"@fr ;
    sh:message "org:siteAddress komt meer dan eens voor of zijn Object is geen schema:PostalAddress"@nl 
  ] .

<#PostShape> a sh:NodeShape;
  sh:targetClass org:Post ;
  sh:property [        
    sh:path org:role ;
    sh:minCount 1 ;
    sh:class org:Role ;

    sh:name "role"@en ;
    sh:name "rôle"@fr ;
    sh:name "rol"@nl;

    sh:description "Indicates the role that the agent plays as a memmber of the organization."@en ;
    sh:description "Indique le rôle de l'agent dans son engagement avec l'organisation."@fr ;
    sh:description "Rol die de agent vervult als lid van de organisatie."@nl ; 

    sh:message "org:role is missing or its Object is no org:Role"@en ;
    sh:message "org:role est absent ou son Object n'est pas un org:Role"@fr ;
    sh:message "org:role ontbreekt of zijn Object is geen org:Role"@nl ;
  ],
  [                
    sh:path org:postIn ;
    sh:minCount 1 ;
    sh:class org:Organization ;

    sh:name "post in"@en ;
    sh:name "poste chez"@fr ;
    sh:name "positie in"@nl ;

    sh:description "Indicates the organization in which the post exists."@en ;
    sh:description "Indique l'organisation dans laquelle le poste existe."@fr ;
    sh:description "Duidt de organisatie aan waarbinnen de positie bestaat."@nl ;

    sh:message "org:postIn is missing or its object is no org:Organization"@en ;
    sh:message "org:postIn est absent ou son object n'est pas un org:Organization"@fr ;
    sh:message "org:postIn ontbreekt of zijn object is geen org:Organization"@nl 
  ] .
    
<#RoleShape> a sh:NodeShape;
  sh:targetClass org:Role ;
  sh:property [ 
    a sh:PropertyShape ;
    sh:path skos:prefLabel ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype rdf:langString ;
      
    sh:name "preferred label"@en ;
    sh:name "label préféré"@fr ;
    sh:name "voorkeursnaam/-label"@nl ;

    sh:description "De voorkeursnaam/-label van de rol."@nl ;
    sh:description "The preferred name/label of the role."@en ;
    sh:description "Le nom / étiquette préféré du rôle."@fr ;
      
    sh:message "skos:prefLabel is missing or its Object is no rdf:langString."@en ;
    sh:message "skos:prefLabel is missing or its Object is no rdf:langString."@fr ;
    sh:message "skos:prefLabel is missing or its Object is no rdf:langString."@nl 

  ] .

<#PersonShape> a sh:NodeShape ;
  sh:targetClass schema:Person ;
  sh:property [    
    sh:path org:holds ;
    sh:class org:Post ;

    sh:name "holds"@en ;
    sh:name "occupe"@fr ;
    sh:name "heeft positie"@nl ;
    
    sh:description "The post that a person holds in the organization."@en ;
    sh:description "Le poste que la personne occupe dans l'organisation."@fr ;
    sh:description "De positie die de persoon binnen de organisatie bekleedt."@nl ;

    sh:message "org:holds is missing or the datatype is no org:Post"@en ;
    sh:message "org:holds est absent ou son Object n'est pas un org:Post"@fr ;
    sh:message "org:holds ontbreekt of het datatype is geen org:Post"@nl
  ],
  [    
    sh:path schema:givenName ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype xsd:string ;

    sh:name "given name"@en ;
    sh:name "prénom"@fr ;
    sh:name "voornaam"@nl;

    sh:description "The given name or first name of this person."@en ;
    sh:description "Le prénom de cette personne."@fr ;
    sh:description "De voornaam van deze persoon."@nl ;
    
    sh:message "schema:givenName is missing or the datatype is no xsd:string."@en ;
  ],
  [    
    sh:path schema:familyName ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype xsd:string ;

    sh:name "family name"@en ;
