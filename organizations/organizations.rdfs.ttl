meemoo:isPartOf a   rdf:Property ;
    rdfs:domain org:Organization ;
    rdfs:range  agr:Agreement ;
    owl:inverseOf meemoo:contains ;
    rdfs:label "is part of"@en ;
    rdfs:label "fait partie de"@fr ;
    rdfs:label "is deel van"@nl .
