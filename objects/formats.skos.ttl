@base <http://data.meemoo.be/ns/terms/format> .

<#pdfa1b> a dct:FileFormat ;   
    rdfs:label "Acrobat PDF/A-1b - Portable Document Format"@en ;   
    premis:version "1b" ;   
    premis:note "Some note about PDF/A-1b" ;   
    skos:exactMatch pronom:354 ;   
    skos:closeMatch     <http://www.digitalpreservation.gov/formats/fdd/fdd000318> .


<#mov> a dct:FileFormat ;
    rdfs:label "Quicktime"@en ;
    skos:exactMatch pronom:658 .

<#m4a> a dct:FileFormat ;
    rdfs:label "Apple Lossless Audio Codec"@en ;
    skos:exactMatch pronom:596 .

<#zip> a dct:FileFormat ;
    rdfs:label "Zip"@en ;
    skos:exactMatch pronomx:263 .