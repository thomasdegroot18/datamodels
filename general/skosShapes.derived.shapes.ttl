<prefLabel>
  rdf:type sh:PropertyShape ;
  sh:path skos:prefLabel ;
  sh:nodeKind sh:Literal ;
  sh:datatype rdf:langString ;
  sh:disjoint skos:altLabel ;
  sh:disjoint skos:hiddenLabel ;
  sh:minCount 1 ;
  sh:name "preferred label"@en ;
  sh:name "étiquette préférée"@fr ;
  sh:name "voorkeurslabel"@nl ;
  sh:uniqueLang "true"^^xsd:boolean ;
  sh:severity sh:Violation ;
.
<altLabel>
  rdf:type sh:PropertyShape ;
  sh:path skos:altLabel ;
  sh:nodeKind sh:Literal ;
  sh:datatype rdf:langString ;
  sh:disjoint skos:hiddenLabel ;
  sh:disjoint skos:prefLabel ;
  sh:message "skos:prefLabel, skos:altLabel and skos:hiddenLabel must be disjoint"@en ;
  sh:message "skos:prefLabel, skos:altLabel et skos:hiddenLabel doivent être disjoint"@fr ;
  sh:message "skos:prefLabel, skos:altLabel en skos:hiddenLabel moeten disjunct zijn"@nl ;
  sh:name "alternative label"@en ;
  sh:name "étiquette alternative"@fr ;
  sh:name "alternatief label"@nl ;
  sh:severity sh:Violation ;
.
<hiddenLabel>
