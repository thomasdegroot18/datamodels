@prefix dct: <http://purl.org/dc/terms/> .
@prefix evtAgRole: <http://id.loc.gov/vocabulary/preservation/eventRelatedAgentRole/> .
@prefix evtObjRole: <http://id.loc.gov/vocabulary/preservation/eventRelatedObjectRole/> .
@prefix evtOutcome: <http://id.loc.gov/vocabulary/preservation/eventOutcome/> .
@prefix evtType: <http://id.loc.gov/vocabulary/preservation/eventType> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix haEvt: <http://data.hetarchief.be/ns/event/> .
@prefix premis: <http://www.loc.gov/premis/rdf/v3/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix schema: <https://schema.org/> .
@prefix pav: <http://purl.org/pav/> .


# prov classes
prov:Activity
    a owl:Class, rdfs:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "Activity"@en ;
    rdfs:label "Activité"@fr ;
    rdfs:label "Activiteit"@nl ;
    owl:disjointWith prov:Entity ;
    skos:definition "An activity is something that occurs over a period of time and acts upon or with entities; it may include consuming, processing, transforming, modifying, relocating, using, or generating entities."@en;
    skos:definition "Une activité est quelque chose qui se passe pendant un certain temps et qui agit sur/avec des entités. Cela peut inclure la consommation, le traitement, la transformation, la modification, le déplacement, l'utilisation ou la génération d'entités."@fr;
    skos:definition "Een activiteit is iets dat zich gedurende een bepaalde periode voordoet en op/met entiteiten handelt. Het kan onder meer volgende zaken inhouden: consumeren, verwerken, transformeren, wijzigen, verplaatsen, gebruiken of genereren van entiteiten."@nl.
    

prov:Agent
    a owl:Class, rdfs:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "Agent"@en ;
    rdfs:label "Agent"@fr ;
    rdfs:label "Agent"@nl ;
    owl:disjointWith prov:InstantaneousEvent ;
    skos:definition "An agent is something that bears some form of responsibility for an activity taking place, for the existence of an entity, or for another agent's activity. "@en;
    skos:definition "Un agent est quelque chose qui porte une forme de responsabilité pour une activité qui se passe, pour l'existence d'une entité ou pour l'activité d'un autre agent. "@fr;
    skos:definition "Een agent is iets dat een bepaalde vorm van verantwoordelijkheid voor een activiteit neemt, voor het bestaan van een entiteit of voor de activiteit van een andere agent. "@nl.
    

prov:Entity
    a owl:Class, rdfs:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "Entity"@en ;
    rdfs:label "Entité"@fr ;
    rdfs:label "Entiteit"@nl ;
    owl:disjointWith prov:InstantaneousEvent ;
    skos:definition "An entity is a physical, digital, conceptual, or other kind of thing with some fixed aspects; entities may be real or imaginary. "@en;
    skos:definition "Une entité est un objet physique, numérique, conceptuel ou autre de type fixe ; les entités peuvent être réelles ou imaginaires."@fr;
    skos:definition "Een entiteit is een fysiek, digitaal, conceptueel of een ander soort ding met enkele vaste aspecten; entiteiten kunnen echt of denkbeeldig zijn."@nl.

# prov properties
prov:generated
    a owl:ObjectProperty ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "generated"@en ;
    rdfs:label "a généré"@fr ;
    rdfs:label "genereerde"@nl ;
    rdfs:range prov:Entity ;
    rdfs:subPropertyOf prov:influenced ;
    owl:inverseOf prov:wasGeneratedBy .

prov:generatedAtTime
    a owl:DatatypeProperty ;
    rdfs:domain prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "generated at time"@en ;
    rdfs:label "a généré au moment"@fr ;
    rdfs:label "genereerde op tijdstip"@nl ;
    rdfs:range xsd:dateTime .

prov:wasGeneratedBy
    a owl:ObjectProperty ;
    rdfs:domain prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "was generated by"@en ;
    rdfs:label "a été généré par"@fr ;
    rdfs:label "is gegenereerd door"@nl ;
    rdfs:range prov:Activity ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom (prov:qualifiedGeneration
        prov:activity
    ) .

prov:invalidatedAtTime
    a owl:DatatypeProperty ;
    rdfs:label "invalidated at time"@en ;
    rdfs:label "invalidé au moment"@fr ;
    rdfs:label "ongeldig gemaakt op tijdstip"@nl ;
    skos:definition "The time at which an entity was invalidated (i.e., no longer usable)."@en ;
    skos:definition "Le moment où une entité a été invalidée (c'est-à-dire n'est plus utilisable)."@fr ;
    skos:definition "Het tijdstip waarop een entiteit ongeldig werd gemaakt (d.w.z. niet langer bruikbaar)."@nl ;
    rdfs:domain prov:Entity ;
    rdfs:range xsd:dateTime ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> .


prov:startedAtTime
    a owl:DatatypeProperty ;
    rdfs:label "started at time"@en ;
    rdfs:label "démarré à l'heure"@fr ;
    rdfs:label "gestart op tijdstip"@nl ;
    skos:definition "The time at which an activity started. See also `prov:endedAtTime`."@en ;
    skos:definition "Le moment où une activité a commencé. Voir aussi `prov:endedAtTime`."@fr ;
    skos:definition "Het tijdstip waarop een activiteit begon. Zie ook `prov:endedAtTime`."@nl ;
    rdfs:domain prov:Activity ;
    rdfs:range xsd:dateTime ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> .

prov:wasAssociatedWith
    a owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:label "was associated with"@en ;
    rdfs:label "a été associé à"@fr ;
    rdfs:label "is geassocieerd met"@nl ;
    skos:definition "An `prov:Agent` that had some (unspecified) responsibility for the occurrence of this `prov:Activity`."@en ;
    skos:definition "Un `prov:Agent` qui avait une responsabilité (non spécifiée) avant la survenue de cette `prov:Activity`."@fr ;
    skos:definition "Een `prov:Agent` die enige (niet-gespecificeerde) verantwoordelijkheid had voor het optreden van deze `prov:Activity`."@nl ;
    rdfs:domain prov:Activity ;
    rdfs:range prov:Agent ;
    owl:propertyChainAxiom (prov:qualifiedAssociation
        prov:agent
    );
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> .

prov:wasAttributedTo
    a owl:ObjectProperty ;
    rdfs:label "was attributed to"@en ;
    rdfs:label "a été attribué à"@fr ;
    rdfs:label "is toegekend aan"@nl ;
    skos:definition "Attribution is the ascribing of an entity to an agent."@en ;
    skos:definition "Attribution est l'attribution d'une entité à un agent."@fr ;
    skos:definition "Toekenning is het toekennen van een entiteit aan een agent."@nl ;
    rdfs:domain prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range prov:Agent ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom (prov:qualifiedAttribution
        prov:agent
    ) .

prov:used
    a owl:ObjectProperty ;
    rdfs:label "used"@en ;
    rdfs:label "a utilisé"@fr ;
    rdfs:label "gebruikte"@nl ;
    skos:definition "A `prov:Entity` that was used by this `prov:Activity`. For example, `:baking prov:used :spoon, :egg, :oven .`"@en ;
    skos:definition "Une `prov:Entity` qui a été utilisée par cette `prov:Activity`. Par exemple , `:baking prov:used :cuillère , :œuf , :four.`"@fr ;
    skos:definition "Een `prov:Entity` die door deze `prov:Activity` werd gebruikt. Bijvoorbeeld: `:bakken prov:used :lepel ,:ei, :oven.`"@nl ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:range prov:Entity ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom (prov:qualifiedUsage
        prov:entity
    ) .

# premis classes

premis:Agent  a owl:Class, rdfs:Class ;
  rdfs:label "Agent"@en ;
  rdfs:label "Acteur"@fr ;
  rdfs:label "Agent"@nl ;
  skos:definition """Actor (human, machine, or software) associated with one or more Event and/or Rights statement associated with a digital object."""@en ;
  skos:definition """Acteur (humain, machine ou logiciel) associé à un ou plusieurs événements (Events) et/ou déclaration(s) de droits associé(s) à un objet numérique."""@fr ;
  skos:definition """Agens (mens, machine of software) geassocieerd met één of meer gebeurtenissen (Events) en/of rechtenuitdrukking(en) geassocieerd met een digitaal object."""@nl ;
  rdfs:subClassOf prov:Agent, foaf:Agent ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> .

premis:SoftwareAgent
  a owl:Class, rdfs:Class ;
  rdfs:label "Software agent"@en ;
  rdfs:label "Logiciel acteur"@fr ;
  rdfs:label "Software agent"@nl ;
  skos:definition """Software agent (e.g., a computer program) that is associated with one or more Event and/or Rights statement associated with a digital object."""@en ;
  skos:definition """Agent logiciel (par ex. un programme informatique) associé à un ou plusieurs événements (Events) et/ou déclaration(s) de droits associé(s) à un objet numérique."""@fr ;
  skos:definition """Softwareagent (bv. een computerprogramma) die gekoppeld is aan één of meer gebeurtenissen (Events) en/of rechtenuitdrukking(en) geassocieerd met een digitaal object."""@nl ;
  rdfs:subClassOf premis:Agent, prov:SoftwareAgent ;
  owl:disjointWith premis:HardwareAgent, premis:Organization, premis:Person ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> .

premis:HardwareAgent
  a owl:Class, rdfs:Class ;
  rdfs:label "Hardware agent"@en ;
  rdfs:label "Matériel acteur"@fr ;
  rdfs:label "Hardware agent"@nl ;
  skos:definition """Hardware agent (e.g., a computer) that is associated with one or more Events and/or Rights statement associated with a digital object."""@en ;
  skos:definition """Agent matériel (par ex. un ordinateur) associé à un ou plusieurs événements (Events) et/ou déclaration(s) de droits associé(s) à un objet numérique."""@fr ;
  skos:definition """Hardwareagent (bv. een computer) die gekoppeld is aan één of meer gebeurtenissen (Events) en/of rechtenuitdrukking(en) geassocieerd met een digitaal object."""@nl ;
  rdfs:subClassOf premis:Agent ;
  owl:sameAs <http://id.loc.gov/vocabulary/preservation/agentType/har> ;
  owl:disjointWith premis:Organization, premis:Person, premis:SoftwareAgent ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> .

premis:Event  a owl:Class, rdfs:Class ;
  rdfs:label "Event"@en ;
  rdfs:label "Événement"@fr ;
  rdfs:label "Gebeurtenis"@nl ;
  skos:definition "Action performed within or outside the repository that affects its capability to preserve Objects over the long term."@en ;
  skos:definition "Action réalisée à l'intérieur ou à l'extérieur du dépôt qui affecte sa capacité à préserver des objets sur le long terme."@fr ;
  skos:definition "Handeling uitgevoerd binnen of buiten het archief die het vermogen beïnvloedt om objecten op lange termijn te bewaren."@nl ;
  rdfs:subClassOf prov:Activity ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> ;
  rdfs:seeAlso <http://id.loc.gov/vocabulary/preservation/eventType> .

premis:OutcomeStatus  a owl:Class, rdfs:Class ;
  rdfs:label "Outcome status"@en ;
  rdfs:label "Statut de résultat"@fr ;
  rdfs:label "Uitkomststatus"@nl ;
  skos:definition "Overall result of the Event in terms of success, partial success, or failure."@en ;
  skos:definition "Résultat global de l'événement en termes de succès, de succès partiel ou d'échec."@fr ;
  skos:definition "Algemeen resultaat van het Event in termen van succes, gedeeltelijk succes of falen."@nl ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> ;
  rdfs:seeAlso <http://id.loc.gov/vocabulary/preservation/eventOutcome> .

# premis properties

premis:endDate  a owl:DatatypeProperty ;
  rdfs:label "end date"@en ;
  rdfs:label "date de fin"@fr ;
  rdfs:label "einddatum"@nl ;
  rdfs:domain [
    a owl:Class, rdfs:Class ;
    owl:unionOf (
     premis:RightsStatus
     premis:Rule
   )
  ] ;
  rdfs:subPropertyOf dct:date ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> .

premis:note  a owl:DatatypeProperty ;
  rdfs:label "has note"@en ;
  rdfs:label "a note"@fr ;
  rdfs:label "heeft nota"@nl ;
  rdfs:range rdfs:Literal ;
  rdfs:subPropertyOf dct:description ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> .

premis:outcome  a owl:ObjectProperty ;
  rdfs:label "has outcome"@en ;
  rdfs:label "a résultat"@fr ;
  rdfs:label "heeft resultaat"@nl ;
  rdfs:domain premis:Event ;
  rdfs:range premis:OutcomeStatus ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> .

premis:outcomeNote  a owl:DatatypeProperty ;
  rdfs:label "has outcome note"@en ;
  rdfs:label "a note de résultat"@fr ;
  rdfs:label "heeft resultaatnota"@nl ;
  rdfs:domain premis:Event ;
  rdfs:range rdfs:Literal ;
  rdfs:subPropertyOf premis:note ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> .

premis:startDate    a owl:DatatypeProperty ;
  rdfs:label "start date"@en ;
  rdfs:label "date de début"@fr ;
  rdfs:label "startdatum"@nl ;
  rdfs:domain [
    a owl:Class, rdfs:Class ;
    owl:unionOf (
     premis:RightsStatus
     premis:Rule
   )
  ] ;
  rdfs:subPropertyOf dct:date ;
  rdfs:isDefinedBy <http://www.loc.gov/premis/rdf/v3/> .

# premis events classes

# premis events properties

evtObjRole:sou a owl:ObjectProperty ;
    rdfs:domain premis:Event ;
    rdfs:range premis:Object ;
    rdfs:label "source"@en;
    rdfs:label "source"@fr;
    rdfs:label "bron"@nl;
    rdfs:subPropertyOf prov:used ;
    skos:definition "The object in relation to the described event that existed prior to that event and was used by it."@en;
    skos:definition "L'objet dans la relation à l'événement (Event) décrit et qui existait avant cet événement (Event) et qui a été utilisé par celui-ci."@fr;
    skos:definition "Het object in relatie tot de beschreven gebeurtenis (Event) dat bestond voor die gebeurtenis (Event) en die erdoor gebruikt werd."@nl;
    rdfs:isDefinedBy <http://id.loc.gov/vocabulary/preservation/eventRelatedObjectRole/>.

evtObjRole:out a owl:ObjectProperty ;
    rdfs:domain premis:Event ;
    rdfs:range premis:Object ;
    rdfs:subPropertyOf prov:generated ;
    rdfs:label "outcome"@en ;
    rdfs:label "résultat"@fr ;
    rdfs:label "uitkomst"@nl;
    skos:definition "The object in relation to the described event is the outcome of that event."@en ;
    skos:definition "L'objet dans la relation à l'événement (Event) décrit est le résultat de cet événement (Event)."@fr ;
    skos:definition "Het object in relatie tot de beschreven gebeurtenis (Event) is het resultaat van die gebeurtenis (Event)."@nl ;
    rdfs:isDefinedBy <http://id.loc.gov/vocabulary/preservation/eventRelatedObjectRole/> .
    
