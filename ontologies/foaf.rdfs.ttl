@prefix foaf:<http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

#####################################
#             PROPERTIES            #
#####################################

foaf:homepage   a   rdf:Property ;
    rdfs:domain owl:Thing ;
    rdfs:range foaf:Document ;
    rdfs:label "Homepage"@en ;
    rdfs:label "Page d'accueil"@fr ;
    rdfs:label "Homepagina"@nl ;
    skos:definition "A homepage for some thing. "@en ;
    skos:definition "Une page d'accueil pour quelque chose"@fr ;
    skos:definition "Een homepagina van/voor iets"@nl ;
    rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/> .